# Portal environment variables (Phase 4 monitoring)
# Copy to .env.local and fill with real values.

# Sentry
SENTRY_DSN=https://examplePublicKey@o0.ingest.sentry.io/0
SENTRY_RELEASE=0.1.0
SENTRY_ENABLED=true
NODE_ENV=production

# Cache TTL overrides (optional, seconds)
CACHE_TTL_SHORT=60
CACHE_TTL_MEDIUM=300
CACHE_TTL_LONG=600
CACHE_TTL_VERY_LONG=1800

# Database (replace with real connection string)
DATABASE_URL=postgresql://user:password@host:5432/dbname?schema=public

# NextAuth / OAuth (placeholders)
NEXTAUTH_SECRET=replace_me
GITHUB_ID=replace_me
GITHUB_SECRET=replace_me
GOOGLE_ID=replace_me
GOOGLE_SECRET=replace_me
